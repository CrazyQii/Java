package test01;

import java.util.Scanner;

public class Demo06 {
	public static void main(String[] args) {
		int a;
		int area;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("输入正方形边长" + a);
		area = area(a);
		System.out.println("正方形面积为：" + area);
	}
	
	public int area(int length) {
		return length * length;
	}

}

