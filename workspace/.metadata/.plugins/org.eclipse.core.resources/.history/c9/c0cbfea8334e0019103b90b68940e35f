package carzyqiqi.day02.demo03;

//导入数组集合
import java.util.ArrayList;

public class DemoArrayList {
	public static void main(String[] args) {
		//创建动态字符串数组集合
		ArrayList<String> list = new ArrayList<>();
		//添加元素
		list.add("apple");
		list.add("orange");
		list.add("banana");
		System.out.println(list);
	}
}
